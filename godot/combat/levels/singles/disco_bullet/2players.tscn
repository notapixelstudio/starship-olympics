[sub_resource type="GDScript" id="3"]
script/source = "extends Node2D

func activate():
	switch(true)
	
func deactivate():
	switch(false)
	
func switch(on):
	for laser in get_children():
		laser.set_on(on)
		if on:
			laser.visible = true
			await get_tree().create_timer(1).timeout
		else:
			await get_tree().create_timer(0.5).timeout
			laser.visible = false
			await get_tree().create_timer(0.5).timeout
		
func show():
	visible = true
	
"

