[sub_resource type="GDScript" id=1]
script/source = "extends Node2D

var tiles := []
var sources := []
var sinks := []

func _ready():
	Events.connect('sth_conquered', self, '_on_sth_conquered')
	
	for child in get_children():
		if child is PlayerSide:
			if child.is_source():
				sources.append(child)
			else:
				sinks.append(child)
		elif child is Tile:
			tiles.append(child)
	
func _on_sth_conquered(conqueror, conquered):
	recompute()
	
func recompute():
	for tile in tiles:
		tile.set_off()
	for sink in sinks:
		sink.set_off()
	for source in sources:
		source.propagate()
"

